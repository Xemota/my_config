#-----------------------------------------------------------------------#
#######################   List of aliases   ############################
#-----------------------------------------------------------------------#

alias ls='ls --color=auto'
alias ..='cd ..'
alias la='ls -a'
alias ll='ls -la'
alias gitshare='f(){ git add "$1" ; 
		     git commit "$1" ; 
                     git push origin master ;  
                     unset -f f; 
                   }; f'
